{
  "name": "openfin-fdc3",
  "version": "0.0.5",
  "description": "OpenFin FDC3 API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "check": "gts check",
    "clean": "gts clean && rimraf dist staging",
    "compile": "npm run compile:client && npm run compile:provider",
    "compile:client": "tsc -p tsconfig.client.json && genversion dist/version.js",
    "compile:provider": "tsc -p tsconfig.provider.json",
    "fix": "gts fix",
    "prepare": "npm run compile && copyfiles package.json dist/",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "package": "webpack --mode development",
    "build": "npm run compile && npm run package",
    "build:demo": "npm run build && cd ./src/demo && npm install && npm run build",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HadoukenIO/fdc3-service.git"
  },
  "author": "OpenFin",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^10.5.2",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "copy-webpack-plugin": "^4.5.2",
    "copyfiles": "^2.0.0",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "genversion": "^2.1.0",
    "gts": "^0.8.0",
    "hadouken-js-adapter": "^0.32.1-alpha.5",
    "mini-css-extract-plugin": "^0.4.1",
    "npm": "^5.7.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.4.2",
    "typescript": "^2.9.2",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8"
  },
  "bugs": {
    "url": "https://github.com/HadoukenIO/fdc3-service/issues"
  },
  "homepage": "https://github.com/HadoukenIO/fdc3-service#readme"
}
