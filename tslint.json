{
    "extends": [
        "gts/tslint.json",
        "tslint-react/tslint-react.json"
    ],
    "rules": {
        "array-type": [
            true,
            "array"
        ],
        "interface-name": false,
        "member-access": true,
        "variable-name": {
            "options": [
                "allow-pascal-case"
            ]
        },
        "no-inferrable-types": false,
        "jsx-no-multiline-js": false
    }
}